[alias]
	lastpatch = log  -p HEAD^1..HEAD
	tree = log --graph --oneline --all --decorate=short
	t = tree
	l = t
	st = status
	s = st
	wdiff = diff --color-words
	bdiff = diff --color-words=.
	d = diff ^HEAD
	ca = commit -av
	c = commit -v
	ca = c
	co = c
	ap = add -p
	deltalog = log --oneline origin/master..HEAD
	pff = pull --ff-only
	mff = merge --ff-only
	ff = mff
